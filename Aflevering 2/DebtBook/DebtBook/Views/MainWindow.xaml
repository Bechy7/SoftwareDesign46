<Window x:Class="DebtBook.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DebtBook.Views"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        xmlns:views="clr-namespace:DebtBook.Views"
        mc:Ignorable="d"
        Title="Debtbook" Height="350" Width="525">
    <StackPanel Orientation="Horizontal">
        <StackPanel DockPanel.Dock="Left" VerticalAlignment="Center">
            <Button Content="Show OverView" Command="{Binding NavigateCommand}" CommandParameter="OverView" Margin="10"/>
            <!--<Button Content="Change to DebitView" Command="{Binding NavigateCommand}" CommandParameter="DebitView" Margin="10"/>-->
        </StackPanel>
        <ContentControl prism:RegionManager.RegionName="ContentRegion">
        </ContentControl>
        <!--<ContentControl>
            <views:OverView />
        </ContentControl>-->
        <!--<Button VerticalAlignment="Center" HorizontalAlignment="Center" Content="Add new debitor" Command="{Binding AddDebitorCommand}"/>-->
    </StackPanel>
</Window>
